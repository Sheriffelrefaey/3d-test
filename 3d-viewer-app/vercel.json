{
  "buildCommand": "npm run build",
  "framework": "nextjs",
  "installCommand": "npm ci",
  "outputDirectory": ".next",
  "ignoreCommand": "git diff HEAD^ HEAD --quiet .",
  "github": {
    "silent": false,
    "autoJobCancelation": true
  },
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1",
      "CI": "true"
    }
  }
}